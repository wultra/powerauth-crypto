<?xml version="1.0" encoding="UTF-8" standalone="no"?><wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:sch="http://getlime.io/security/powerauth" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://getlime.io/security/powerauth" targetNamespace="http://getlime.io/security/powerauth">
  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://getlime.io/security/powerauth" version="1.0">
           
   	<!-- Status method //-->
    
    <xs:element name="GetSystemStatusRequest">
        <xs:annotation>
        	<xs:documentation>Request for getting the system status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
    		<xs:sequence/>
    	</xs:complexType>
    </xs:element>
    
    <xs:element name="GetSystemStatusResponse">
        <xs:annotation>
        	<xs:documentation>Response for getting the system status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
        	<xs:sequence>
            	<xs:element maxOccurs="1" minOccurs="1" name="status" type="xs:string"/>
	            <xs:element maxOccurs="1" minOccurs="1" name="applicationName" type="xs:string"/>
	            <xs:element maxOccurs="1" minOccurs="1" name="applicationDisplayName" type="xs:string"/>
	            <xs:element maxOccurs="1" minOccurs="1" name="applicationEnvironment" type="xs:string"/>
	            <xs:element maxOccurs="1" minOccurs="1" name="timestamp" type="xs:dateTime"/>
        	</xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Server error code list -->
    
    <xs:element name="GetErrorCodeListRequest">
        <xs:annotation>
        	<xs:documentation>Request for getting the list of all possible server error codes.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
    		<xs:sequence>
    			<xs:element maxOccurs="1" minOccurs="1" name="language" type="xs:string"/>
    		</xs:sequence>
    	</xs:complexType>
    </xs:element>
    
    <xs:element name="GetErrorCodeListResponse">
        <xs:annotation>
        	<xs:documentation>Response for getting the list of all possible server error codes.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
        	<xs:sequence>
            	<xs:element maxOccurs="unbounded" minOccurs="0" name="errors">
            		<xs:complexType>
        	        	<xs:sequence>
    	       				<xs:element maxOccurs="1" minOccurs="1" name="code" type="xs:string"/>
	            			<xs:element maxOccurs="1" minOccurs="1" name="value" type="xs:string"/>
           				</xs:sequence>
        			</xs:complexType>
        		</xs:element>
        	</xs:sequence>
        </xs:complexType>
    </xs:element>
           
    <!-- List applications //-->
    
    <xs:element name="GetApplicationListRequest">
        <xs:annotation>
        	<xs:documentation>Request for getting the application list.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
    		<xs:sequence/>
    	</xs:complexType>
    </xs:element>
    
    <xs:element name="GetApplicationListResponse">
        <xs:annotation>
        	<xs:documentation>Response for getting the application list.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
        	<xs:sequence>
            	<xs:element maxOccurs="unbounded" minOccurs="0" name="applications">
            		<xs:complexType>
        	        	<xs:sequence>
    	       				<xs:element maxOccurs="1" minOccurs="1" name="id" type="xs:long"/>
	            			<xs:element maxOccurs="1" minOccurs="1" name="applicationName" type="xs:string"/>
           				</xs:sequence>
        			</xs:complexType>
        		</xs:element>
        	</xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Get application details //-->
    
    <xs:element name="GetApplicationDetailRequest">
        <xs:annotation>
        	<xs:documentation>Request for getting the application detail, including the list of versions.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
    		<xs:sequence>
    			<xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
    		</xs:sequence>
    	</xs:complexType>
    </xs:element>
    
    <xs:element name="GetApplicationDetailResponse">
        <xs:annotation>
        	<xs:documentation>Response for getting the application detail, including the list of versions.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
        	<xs:sequence>
        		<xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
        		<xs:element maxOccurs="1" minOccurs="1" name="applicationName" type="xs:string"/>
        		<xs:element maxOccurs="1" minOccurs="1" name="masterPublicKey" type="xs:string"/>
        		<xs:element maxOccurs="unbounded" minOccurs="0" name="versions">
                	<xs:complexType>
                	    <xs:sequence>
            	            <xs:element maxOccurs="1" minOccurs="1" name="applicationVersionId" type="xs:long"/>
                       		<xs:element maxOccurs="1" minOccurs="1" name="applicationVersionName" type="xs:string"/>
    	                   	<xs:element maxOccurs="1" minOccurs="1" name="applicationKey" type="xs:string"/>
	               			<xs:element maxOccurs="1" minOccurs="1" name="applicationSecret" type="xs:string"/>
                			<xs:element maxOccurs="1" minOccurs="1" name="supported" type="xs:boolean"/>
                       	</xs:sequence>
                   	</xs:complexType>
               	</xs:element>
        	</xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Get application details by application key //-->

    <xs:element name="LookupApplicationByAppKeyRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the application detail based on the application version key.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationKey" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="LookupApplicationByAppKeyResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the application detail based on the application version key.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Create application //-->
    
    <xs:element name="CreateApplicationRequest">
        <xs:annotation>
        	<xs:documentation>Request for creating a new activation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationName" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="CreateApplicationResponse">
        <xs:annotation>
        	<xs:documentation>Response for creating a new activation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationName" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Create application version //-->
    
    <xs:element name="CreateApplicationVersionRequest">
        <xs:annotation>
        	<xs:documentation>Request for creating a new activation version.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationVersionName" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="CreateApplicationVersionResponse">
        <xs:annotation>
        	<xs:documentation>Response for creating a new activation version.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationVersionId" type="xs:long"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationVersionName" type="xs:string"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationKey" type="xs:string"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationSecret" type="xs:string"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="supported" type="xs:boolean"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Unsupport application version //-->
    
    <xs:element name="UnsupportApplicationVersionRequest">
        <xs:annotation>
        	<xs:documentation>Request for marking a given application version as unsupported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationVersionId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="UnsupportApplicationVersionResponse">
        <xs:annotation>
        	<xs:documentation>Response for marking a given application version as unsupported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationVersionId" type="xs:long"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="supported" type="xs:boolean"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Support application version //-->
    
    <xs:element name="SupportApplicationVersionRequest">
        <xs:annotation>
        	<xs:documentation>Request for marking a given application version as supported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationVersionId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="SupportApplicationVersionResponse">
        <xs:annotation>
        	<xs:documentation>Response for marking a given application version as supported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
            	<xs:element maxOccurs="1" minOccurs="1" name="applicationVersionId" type="xs:long"/>
            	<xs:element maxOccurs="1" minOccurs="1" name="supported" type="xs:boolean"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Init activation //-->
    
    <xs:element name="InitActivationRequest">
        <xs:annotation>
        	<xs:documentation>Request for initiating the activation process by generating the activation code.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
                <xs:element maxOccurs="1" minOccurs="0" name="timestampActivationExpire" type="xs:dateTime"/>
                <xs:element maxOccurs="1" minOccurs="0" name="maxFailureCount" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="InitActivationResponse">
        <xs:annotation>
        	<xs:documentation>Response for initiating the activation process.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
            	<xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationIdShort" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationOTP" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationSignature" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Exchange public keys //-->
    
    <xs:element name="PrepareActivationRequest">
        <xs:annotation>
        	<xs:documentation>Request for client-server key exchange procedure.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationIdShort" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="activationName" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="extras" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationNonce" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="ephemeralPublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedDevicePublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationSignature" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="PrepareActivationResponse">
        <xs:annotation>
        	<xs:documentation>Response for client-server key exchange procedure.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationNonce" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="ephemeralPublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedServerPublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedServerPublicKeySignature" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Create new activation directly //-->

    <xs:element name="CreateActivationRequest">
        <xs:annotation>
            <xs:documentation>Request for creating a new activation directly.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
                <xs:element maxOccurs="1" minOccurs="0" name="timestampActivationExpire" type="xs:dateTime"/>
                <xs:element maxOccurs="1" minOccurs="0" name="maxFailureCount" type="xs:long"/>
                <xs:element maxOccurs="1" minOccurs="1" name="identity" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationOtp" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="activationName" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="extras" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationNonce" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="ephemeralPublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedDevicePublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationSignature" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CreateActivationResponse">
        <xs:annotation>
            <xs:documentation>Response for creating a new activation directly.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationNonce" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="ephemeralPublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedServerPublicKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedServerPublicKeySignature" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Commit activation //-->
    
    <xs:element name="CommitActivationRequest">
        <xs:annotation>
        	<xs:documentation>Request for committing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="CommitActivationResponse">
        <xs:annotation>
        	<xs:documentation>Response for committing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activated" type="xs:boolean"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Get Activation Status //-->
    
    <xs:element name="GetActivationStatusRequest">
        <xs:annotation>
        	<xs:documentation>Request for getting the activation details / status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetActivationStatusResponse">
        <xs:annotation>
        	<xs:documentation>Response for getting the activation details / status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationStatus" type="tns:ActivationStatus"/>
                <xs:element maxOccurs="1" minOccurs="0" name="activationName" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="extras" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
                <xs:element maxOccurs="1" minOccurs="1" name="timestampCreated" type="xs:dateTime"/>
                <xs:element maxOccurs="1" minOccurs="1" name="timestampLastUsed" type="xs:dateTime"/>
                <xs:element maxOccurs="1" minOccurs="1" name="encryptedStatusBlob" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="activationIdShort" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="activationOTP" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="activationSignature" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="devicePublicKeyFingerprint" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Remove Activation //-->
    
    <xs:element name="RemoveActivationRequest">
        <xs:annotation>
        	<xs:documentation>Request for removing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RemoveActivationResponse">
        <xs:annotation>
        	<xs:documentation>Response for removing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="removed" type="xs:boolean"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Activations for user //-->
    
    <xs:element name="GetActivationListForUserRequest">
        <xs:annotation>
        	<xs:documentation>Request for getting the list of activations for given user.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="0" name="applicationId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetActivationListForUserResponse">
        <xs:annotation>
        	<xs:documentation>Response for getting the list of activations for given user.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="activations">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                            <xs:element maxOccurs="1" minOccurs="1" name="activationStatus" type="tns:ActivationStatus"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="activationName" type="xs:string"/>
                			<xs:element maxOccurs="1" minOccurs="0" name="extras" type="xs:string"/>
                			<xs:element maxOccurs="1" minOccurs="1" name="timestampCreated" type="xs:dateTime"/>
                			<xs:element maxOccurs="1" minOccurs="1" name="timestampLastUsed" type="xs:dateTime"/>
                			<xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                			<xs:element maxOccurs="1" minOccurs="1" name="applicationId" type="xs:long"/>
                			<xs:element maxOccurs="1" minOccurs="1" name="applicationName" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Verify signature //-->
    
    <xs:element name="VerifySignatureRequest">
        <xs:annotation>
        	<xs:documentation>Request for the signature verification process.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="applicationKey" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="data" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="signature" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="signatureType" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="VerifySignatureResponse">
        <xs:annotation>
        	<xs:documentation>Response for the signature verification process.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="userId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationStatus" type="tns:ActivationStatus"/>
                <xs:element maxOccurs="1" minOccurs="1" name="remainingAttempts" type="xs:integer"/>
                <xs:element maxOccurs="1" minOccurs="1" name="signatureValid" type="xs:boolean"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Block Activation //-->
    
    <xs:element name="BlockActivationRequest">
        <xs:annotation>
        	<xs:documentation>Request for blocking the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="BlockActivationResponse">
        <xs:annotation>
        	<xs:documentation>Response for blocking the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
                <xs:element maxOccurs="1" minOccurs="1" name="activationStatus" type="tns:ActivationStatus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <!-- Unblock Activation //-->
    
    <xs:element name="UnblockActivationRequest">
        <xs:annotation>
        	<xs:documentation>Request for unblocking the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="activationId" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="UnblockActivationResponse">
        <xs:annotation>
        	<xs:documentation>Response for unblocking the activation with given ID.</xs:documentation>
